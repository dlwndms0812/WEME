<form class="container" action="/Login/index" method="get">
    <div class="md-4">
        <label class="form-label"> 아이디 </label>
        <input type="text" class="form-control" name="id">
    </div>
    <div class="mb-4">
        <label class="form-label">제목</label>
        <input type="text" class="form-control" name="title">
    </div>
    <div class="md-4">
        <label class="form-label"> 사진 </label>
        <input type="text" class="form-control" name="image">
    </div>
    <div class="md-4">
        <label class="form-label">동물 이름</label>
        <input type="text" class="form-control" name="animal_name">
    </div>
    <div class="md-4">
        <label class="form-label"> 동물 종류 </label>
        <input type="text" class="form-control" name="animal_kinds">
    </div>
    <div class="md-4">
        <label class="form-label"> 동물 성별 </label>
        <input type="text" class="form-control" name="animal_gender">
    </div>
    <div class="md-4">
        <label class="form-label"> 동물 크기 </label>
        <input type="text" class="form-control" name="animal_size">
    </div>
    <div class="md-4">
        <label class="form-label"> 동물 특징 </label>
        <input type="text" class="form-control" name="animal_feature">
    </div>
    <div class="md-4">
        <label class="form-label"> 동물 나이 </label>
        <input type="text" class="form-control" name="animal_age">
    </div>
    <div class="md-4">
        <label class="form-label"> 동물 예방접종 </label>
        <input type="text" class="form-control" name="animal_vaccine">
    </div>
    <div class="md-4">
        <label class="form-label"> 동물 중성화 </label>
        <input type="text" class="form-control" name="animal_neuter">
    </div>
    <div class="md-4">
        <label class="form-label"> 지역1 </label>
        <input type="text" class="form-control" name="animal_area1">
    </div>
    <div class="md-4">
        <label class="form-label"> 지역2 </label>
        <input type="text" class="form-control" name="animal_area2">
    </div>
    <div class="md-4">
        <label class="form-label"> 글 내용 </label>
        <input type="text" class="form-control" name="context">
    </div>
    <div class="md-4">
        <label class="form-label"> 날짜 </label>
        <input type="text" class="form-control" name="posts_date">
    </div>
    <div class="md-4">
        <label class="form-label"> 보드 아이디 </label>
        <input type="text" class="form-control" name="board_id">
    </div>
    <div class="md-4">
        <label class="form-label"> 포스트 숫자 </label>
        <input type="text" class="form-control" name="posts_num">
    </div>
    <div class="md-4">
        <label class="form-label"> 포스트 url </label>
        <input type="text" class="form-control" name="posts_url">
    </div>
    <button type="submit" class="btn btn-primary">게시글 등록하기</button>
</form>
